	scwin.SI0801G0_onrowindexchange = function(row,oldRow) {
		var dataType = dlt_BMS_FCLT_PARAM_CFG_INFO.getCellData(row, "DATA_TYPE");
		
		switch (dataType) {
		case "DT001":
			//$p.$("#DATA_VAL").attr("allowChar", "");
			DATA_VAL.setAllowChar("0-9");
			DATA_VAL.setMaxLength(10);
		break;
		case "DT002":
			DATA_VAL.setAllowChar("")
			DATA_VAL.setMaxLength(20);
		break;
		case "DT003":
			DATA_VAL.setAllowChar("0-9")
			DATA_VAL.setMaxLength(10);
		break;
		case "DT004":
			DATA_VAL.setAllowChar("")
			DATA_VAL.setMaxLength(5);
		break;
		case "DT005":
			DATA_VAL.setAllowChar("0-9")
			DATA_VAL.setMaxLength(20);
		break;
		case "DT006":
			DATA_VAL.setAllowChar("0-9")
			DATA_VAL.setMaxLength(20);
		break;
		case "DT007":
			DATA_VAL.setAllowChar("0-9")
		break;
		case "DT008":
			DATA_VAL.setAllowChar("")
		break;
		case "DT009":
			DATA_VAL.setAllowChar("")
			DATA_VAL.setMaxLength(5);
		break;
		
		}
		
		var autoYn = dlt_BMS_FCLT_PARAM_CFG_INFO.getCellData(row, "AUTO_YN");
		if(autoYn == 'Y'){
			SI0801G1.setDisabled("grid", true);
			dlt_BMS_FCLT_SCH_INFO.removeAll();
			wfm_subBtn.setDisabled(true);
			wfm_subBtn.setOpacity(0.5);
		}else{
			SI0801G1.setDisabled("grid", false);
			wfm_subBtn.setDisabled(false);
			wfm_subBtn.setOpacity(1);
		}
	
		dma_sub_search.set("FCLT_KIND", dlt_BMS_FCLT_PARAM_CFG_INFO.getCellData(row, "FCLT_KIND"));
		dma_sub_search.set("PARAM_DIV", dlt_BMS_FCLT_PARAM_CFG_INFO.getCellData(row, "PARAM_DIV"));
		dma_sub_search.set("PARAM_KIND", dlt_BMS_FCLT_PARAM_CFG_INFO.getCellData(row, "PARAM_KIND"));
		
		com.executeSubmission(sub_SI0801G1R0);
		
	};
	

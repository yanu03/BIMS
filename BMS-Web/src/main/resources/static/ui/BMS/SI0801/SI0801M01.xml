			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "FCLT_KIND", "FCLT_KIND");
			//검색부분 코드값 변경
			var codeOptions = [ {code : "FCLT_KIND", compID : "autoChoiceBox" } ];
			com.setCommonDtl(codeOptions);
			autoChoiceBox.show("");
		} else if (info.newValue == "PARAM_DIV") {
			search_input.hide();

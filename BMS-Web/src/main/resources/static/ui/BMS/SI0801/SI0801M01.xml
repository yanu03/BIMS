	scwin.subGridUp = function(){
		var idx = SI0801G1.getFocusedRowIndex();
		com.getGridViewDataList(SI0801G1).rowMoveUp(idx);
		dlt_BMS_FCLT_SCH_INFO.setCellData(idx, "SN", idx+1);
		dlt_BMS_FCLT_SCH_INFO.setCellData(idx-1, "SN", idx);
	}
	
	scwin.subGridDown = function(){
		var idx = SI0801G1.getFocusedRowIndex();
		com.getGridViewDataList(SI0801G1).rowMoveDown(idx);
		dlt_BMS_FCLT_SCH_INFO.setCellData(idx+1, "SN", idx+2);
		dlt_BMS_FCLT_SCH_INFO.setCellData(idx, "SN", idx+1);
	}
	
	//서브 추가버튼
	scwin.addSubGrid = function(){
		var rowStatus = SI0801G0.getRowStatus(SI0801G0.getFocusedRowIndex());
		if (rowStatus == "C") {
			com.alert("부모 등록 후에 추가 가능합니다.");
			return;
		}else{
			com.addGrid2(SI0801G1,"ST_TIME");
			var sn = dlt_BMS_FCLT_SCH_INFO.getCellData(SI0801G1.getFocusedRowIndex()-1, "SN");
			if(sn != null && sn != ''){
				dlt_BMS_FCLT_SCH_INFO.setCellData(SI0801G1.getFocusedRowIndex(), "SN", parseInt(sn)+1);
			}else{
				dlt_BMS_FCLT_SCH_INFO.setCellData(SI0801G1.getFocusedRowIndex(), "SN", 1);
			}
			
		}
		
	}
	

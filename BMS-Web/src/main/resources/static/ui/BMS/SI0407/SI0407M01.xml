		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.SINGLE_GRID, tabGrid[idx], null, codeOptions[idx] );
		com.setFocusedCell(tabGrid[idx].Main.grid, tabGrid[idx].Main.keyColumn);

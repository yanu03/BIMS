			[ {code : "ROAD_RANK", compID : "SI0407G1:ROAD_RANK" },{code : "ROAD_TYPE", compID : "SI0407G1:ROAD_TYPE" }
				,{code : "ROAD_USE", compID : "SI0407G1:ROAD_USE" },{code : "MULTI_LINK", compID : "SI0407G1:MULTI_LINK" }
				,{code : "CONNECT", compID : "SI0407G1:CONNECT" },{code : "REST_VEH", compID : "SI0407G1:REST_VEH" }]
		];
		
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.SINGLE_GRID, tabGrid[0], null, codeOptions[0]);	
				 
		//com.executeSubmission(sub_SI0407SHI1);

		com.setCommonDtl(codeOptions[0], function() {

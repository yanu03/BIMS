	}
	
	scwin.update_search_info = function(idx){
		
		
		
		/*for(var i = sbx_ChoiceBox.getItemCount()-1; i >= 1 ; i--){
			sbx_ChoiceBox.deleteItem(i, true);
		}*/
		
		sbx_ChoiceBox.removeAll();
		var placeholderStr = "";
		for(var i = 0; i < searchInfo[idx].length; i++){
			sbx_ChoiceBox.addItem(searchInfo[idx][i].value, searchInfo[idx][i].label, i, true);
			placeholderStr += searchInfo[idx][i].label;
			if( i == 0) placeholderStr = "";
			else if( i == 1) placeholderStr += " 또는 ";
			else if( i < searchInfo[idx].length-1) placeholderStr += ", ";
		}
		sbx_ChoiceBox.setSelectedIndex(0);
		autoChoiceBox.hide();
		
		search_input.setValue("");
		search_input.setPlaceholder(placeholderStr);
		search_input.show("");
	}
	

	scwin.sub_SI0407G0S0_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
			com.saveDone(SI0407G0,sub_SI0407G0R0);
			com.executeSubmission(sub_SI0407SHI0); //저장후 표준링크에서 노드 선택할수 있도록 db에서 목록을 다시 읽어 옴
		}
	};
	
	scwin.sub_SI0407G1R0_submitdone = function(e) {
		com.setFocusedCell(SI0407G1, "MOCK_LINK_ID");
	};
	
	scwin.sub_SI0407G1S0_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
			com.saveDone(SI0407G1,sub_SI0407G1R0);
		}
	};
	
	scwin.sbx_ChoiceBox_onviewchange = function(info) {
		
		if (info.newValue == "ALL") {
			autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} 
		else if (info.newValue == "MOCK_NODE_ID") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "MOCK_NODE_ID", "MOCK_NODE_ID");
			autoChoiceBox.show("");
			
		} else if (info.newValue == "NODE_NAME") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "NODE_NAME", "NODE_NAME");
			autoChoiceBox.show("");
		}
		else if (info.newValue == "MOCK_LINK_ID") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem2", "MOCK_LINK_ID", "MOCK_LINK_ID");
			autoChoiceBox.show("");
			
		} else if (info.newValue == "F_NODE_NAME") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem2", "F_NODE_NAME", "F_NODE_NAME");
			autoChoiceBox.show("");
		}		
		else if (info.newValue == "T_NODE_NAME") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem2", "T_NODE_NAME", "T_NODE_NAME");
			autoChoiceBox.show("");
		}
	};
	
	scwin.SI0407G1_onrowindexchange = function(row,oldRow) {
		linkFocus = row;
		com.changeSingleGrid(SI0407G1, row, oldRow);
	};

	scwin.SI0407G1_onchange = function(row,oldRow) {
		var f_node = dlt_BMS_MOCK_LINK.getCellData(row, "F_NODE");
		var f_node_name = dlt_BMS_MOCK_LINK.getCellData(row, "F_NODE_NAME");
		var t_node = dlt_BMS_MOCK_LINK.getCellData(row, "T_NODE");
		var t_node_name = dlt_BMS_MOCK_LINK.getCellData(row, "T_NODE_NAME");
		
		for(var i = 0; i < dlt_searchitem.getRowCount(); i++){
		
			m_node = dlt_searchitem.getCellData(i, "MOCK_NODE_ID");
			m_node_name = dlt_searchitem.getCellData(i, "NODE_NAME");
			
			if(m_node == f_node && m_node_name !== f_node_name){
				dlt_BMS_MOCK_LINK.setCellData(row, "F_NODE_NAME", m_node_name);
			}

			if(m_node == t_node && m_node_name !== t_node_name){
				dlt_BMS_MOCK_LINK.setCellData(row, "T_NODE_NAME", m_node_name);
			}
		}
				
	};
	


		//var focusOption = {keyMap : dma_subsearch};
		//com.changeDualGrid(VD0101G1, VD0101G2, null, sub_VD0101G2R0, focusOption, row, oldRow);
	};
	
	scwin.sub_VD0101G2R0_submitdone = function(e) {
		com.setFocusedCell(VD0101G2, "DVC_KIND");		
	};
	
	scwin.sub_VD0101G2S0_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
			com.saveDone(VD0101G0,sub_VD0101G0R0);
		}
	};
	
	scwin.useYnDisFormatter = function(data){
		if(data == ""){

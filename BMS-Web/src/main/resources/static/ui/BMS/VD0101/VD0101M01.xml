		 };
		 
		var mainOptions = {
				ADD:scwin.paramPopupOnclick
		}
		 
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.DUAL_GRID3, autoOptions, mainOptions);
		
		var codeOptions = [ {code : "AREA", compID : "VD0101G0:AREA" },
							{code : "VHC_KIND", compID : "VD0101G0:VHC_KIND"},
							{code : "ROUT_TYPE", compID : "VD0101G0:ROUT_TYPE"},
							{code : "VHC_TYPE", compID : "VD0101G0:VHC_TYPE"},
							{code : "VHC_FUEL", compID : "VD0101G0:VHC_FUEL"},
							{code : "DVC_KIND", compID : "VD0101G1:DVC_KIND"},

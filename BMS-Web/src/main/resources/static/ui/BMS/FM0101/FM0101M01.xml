			Sub2 : { grid : FM0101G2, srchSbm : sub_FM0101G2R0, savSbm : sub_FM0101G2S0, name :  "파라미터 목록"}
		 };
		 
		var mainOptions = {
				ADD:scwin.paramPopupOnclick
		}
		 
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.DUAL_GRID3, autoOptions, mainOptions);
		
		var codeOptions = [ {code : "NODE_TYPE", compID : "FM0101G0:NODE_TYPE" },
							{code : "INST_LOC", compID : "FM0101G1:INST_LOC" },
							{code : "FCLT_KIND", compID : "FM0101G1:FCLT_KIND"},
							{code : "FCLT_KIND", compID : "FM0101G2:FCLT_KIND"},
							{code : "PARAM_DIV", compID : "FM0101G2:PARAM_DIV" },
							{code : "PARAM_KIND", compID : "FM0101G2:PARAM_KIND" },
							{code : "DATA_TYPE", compID : "FM0101G2:DATA_TYPE" }
		 ];
		 
		com.setCommonDtl(codeOptions);
						 
		com.executeSubmission(sub_FM0101SHI0);	
			
		
	};

	scwin.onpageunload = function() {

	};
	
	scwin.paramPopupOnclick = function(){
		var focusRowIdx = scwin.idxRowAuthority;
		var dvcKind = dlt_BMS_FCLT_INFO.getCellData(FM0101G1.getFocusedRowIndex(), "FCLT_KIND")
		dma_subsearch.set("FCLT_KIND", dvcKind);
		
		var paramKind = dlt_BMS_FCLT_PARAM_CFG_INFO.getColData("PARAM_KIND");
		dma_subsearch.set("PARAM_ECPT_IDS", paramKind);
		
		if (focusRowIdx == -1) {
			com.alert("장치 선택후 파라미터를 등록하실 수 있습니다.");
		} else {
			var FCLT_KIND = dma_subsearch.get("FCLT_KIND");
			if (FCLT_KIND != null) {
				var data = { data : dma_subsearch.getJSON(), callbackFn : "scwin.insertPopupData" };
				var options = { id : "FM0101P01",
								popupName : "파라미터 추가", 
								modal : true};
				com.openPopup("/ui/BMS/FM0101/FM0101P01.xml", options, data);
			}			
		}
	}
	
	scwin.insertPopupData = function(obj) {
		debugger;
		var dLength = obj.length;
		for (i = 0; i < dLength; i++) {
			var data = dlt_BMS_FCLT_INFO.getMatchedIndex("FCLT_KIND", obj[i].FCLT_KIND, true);
			obj[i].FCLT_ID = dma_subsearch.get("FCLT_ID");
			obj[i].NODE_ID = dma_subsearch.get("NODE_ID");
			obj[i].MNG_ID = dlt_BMS_FCLT_INFO.getCellData(FM0101G1.getFocusedRowIndex(), "MNG_ID");
			obj[i].chk = "0";
			var idx = dlt_BMS_FCLT_PARAM_CFG_INFO.insertRow();
			dlt_BMS_FCLT_PARAM_CFG_INFO.setRowJSON(idx, obj[i], true);
		}
		com.setFocusedCell(FM0101G2, "FCLT_KIND");
		
	};
	

	<script type="text/javascript" lazy="false"><![CDATA[
	scwin.onpageload = function() {
		var autoOptions = {
			Main : { grid : FM0101G0, srchSbm : sub_FM0101G0R0, srchGrp : pgm_header, keyColumn : "NODE_ID", focusColumn : "VHC_NO", name :  "차량 목록"},

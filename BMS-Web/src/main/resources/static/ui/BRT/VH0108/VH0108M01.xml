			acb_autoChoiceBox.show("");
		}
		
	};
	
	//라디오 버튼에 따라 날짜 설정
	scwin.calendar_radio_onviewchange = function(info) {
		var date = new Date();
		var year = date.getFullYear().toString();
		var month = (date.getMonth()+1).toString();
		var day = date.getDate().toString();
		var zero = 0;
		
		
		//글자수 맞추기
		if(day.length == 1){
			day = zero + day;
		}
		
		if(month.length == 1){
			month = zero + month;
		}
		
		var weekAgoDay = parseInt(day) - 7;
		if(weekAgoDay.toString.length == 1){
			weekAgoDay = zero + weekAgoDay.toString();
		}
		var todaysDate = year+month+day;
		var weekAgoDate = year+month+weekAgoDay;
		var firstday = year+month+"01";		
		
		if(parseInt(weekAgoDay) <= 0) {
	         weekAgoDate = firstday;
	    }
		
		if(info.value == "DIRECT") {
			F_DATE.setValue();
			L_DATE.setValue();
		}
		
		else if(info.value == "TODAY") {
			F_DATE.setValue(todaysDate);
			L_DATE.setValue(todaysDate);	
		}
		
		else if(info.value == "WEEK") {
			F_DATE.setValue(weekAgoDate);
			L_DATE.setValue(todaysDate);			
		}
		
		else if(info.value == "MONTH") {
			F_DATE.setValue(firstday);
			L_DATE.setValue(todaysDate);				
		}
	};
	
	scwin.getFooterDiv = function(data, dataType) {
		var sum = 0;
		var count01 = 0;
		var count02 = 0;
		var count03 = 0; 
		var count04 = 0; 
		var count05 = 0; 
		var count06 = 0; 
		var count07 = 0; 
		var count08 = 0; 
		var result = "";
		
		if (data.length > 0) {
			sum = data.length;
			for (var i=0; i<data.length; i++) {
				if(data[i] == "VL001") {
					count01++;
				}
				else if(data[i] == "VL002") {
					count02++;
				}					
				else if(data[i] == "VL003") {
					count03++;
				}
				else if(data[i] == "VL004") {
					count04++;
				}					
				else if(data[i] == "VL005") {
					count05++;
				}
				else if(data[i] == "VL006") {
					count06++;
				}					
				else if(data[i] == "VL007") {
					count07++;
				}
				else if(data[i] == "VL008") {
					count08++;
				}					
			}
			
			result = "총계 : " + sum + "건, 급가속 : " + count01 + "건, 급감속 : " + count02 + "건, 과속 : " + count03 + "건, 무정차통과 : " + count04 + "건, 개문주행 : " + count05 + "건, 급출발 : " + count06 + "건, 노선이탈 : "
				+ count07 + "건, 급정지 : " + count08 + "건";
			return result;
		}
	};	
	

	};*/
	
	scwin.F_DATE_VH0105_onviewchange = function(info) {
		var ldate = L_DATE_VH0105.getValue();
		var fdate = F_DATE_VH0105.getValue();
		calendar_radio.setValue("DIRECT");
		if (fdate > ldate && ldate != ""){
			com.messagBox("alert", "지정할 수 없는 날짜입니다.",scwin.checkCLoseF(info));
		}
	};
	scwin.checkCLoseF = function(info) {
		F_DATE_VH0105.setValue(info.oldValue);
	}
	scwin.checkCLoseL = function(info) {
		L_DATE_VH0105.setValue(info.oldValue);
	}	
	scwin.L_DATE_VH0105_onviewchange = function(info) {
		var ldate = L_DATE_VH0105.getValue();
		var fdate = F_DATE_VH0105.getValue();
		calendar_radio.setValue("DIRECT");
		if (fdate > ldate){
			com.messagBox("alert", "지정할 수 없는 날짜입니다.",scwin.checkCLoseL(info));
		}
	};
	
	//라디오 버튼에 따라 날짜 설정
	scwin.calendar_radio_onviewchange = function(info) {
		com.setCalendarRadioChange(F_DATE_VH0105, L_DATE_VH0105, info);
	};
	
	scwin.getFooterDiv = function(data, dataType) {
		var count01 = 0;
		var count02 = 0;
		var count03 = 0; 
		var total = dlt_BRT_CUR_OPER_HIS.getRowCount();
		var result = "";
		
		if (data.length > 0) {
			for (var i=0; i<data.length; i++) {
				if(data[i] == "OS001") {
					count01++;
				}
				else if(data[i] == "OS002") {
					count02++;
				}					
				else if(data[i] == "OS003") {
					count03++;
				}					
			}
			
			//total = count01 + count02 + count03;
			
			result = "총 : "+ total + "건, 정상운행 : " + count01 + "건, 운행종료 : " + count02 + "건, 차고지행 : " + count03 + "건";
			return result;
		}
		else if(data.length == 0){
		
			result = "총 : "+ total + "건, 정상운행 : " + count01 + "건, 운행종료 : " + count02 + "건, 차고지행 : " + count03 + "건";
			return result;
		}
		
	};		

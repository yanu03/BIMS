	scwin.onpageload = function() {
		 var autoOptions = {
			Main : { grid : VH0105G0, srchSbm : sub_VH0105G0R0, keyColumn : "OPER_DT", srchGrp : pgm_header, name : "현재운행이력 조회",
						focusColumn : "OPER_DT"}
		 };
		 
		com.setMainBtn2(wfm_mainBtn,
		
		gcm.DISP_TYPE.SINGLE_GRID, autoOptions);

		dma_search.F_DATE_VH0105 = util.getCurrentDate2();
		F_DATE_VH0105.setValue(util.getCurrentDate2());
		
		dma_search.L_DATE_VH0105 = util.getCurrentDate2();
		L_DATE_VH0105.setValue(util.getCurrentDate2());
		
		routMap.initMap2("map_VH0105", 
				{width:"100%"
				, height:"100%"
				, onClick: scwin.onClickMap
		});
		
		var codeOptions = [ {code : "OPER_STS", compID : "VH0105G0:OPER_STS" },
							{code : "BUS_STS", compID : "VH0105G0:BUS_STS" }
		 ];			
		 
		com.setCommonDtl(codeOptions, function() {
			com.searchGrid(VH0105G0, sub_VH0105G0R0);
		});

		com.executeSubmission(sub_repRoutItem);
		com.executeSubmission(sub_vhcItem);	
		
	};	
	
	scwin.VH0105G0_onrowindexchange = function(row,oldRow) {

		dma_search.set("ROUT_ID", dlt_BRT_CUR_OPER_HIS.getCellData(VH0105G0.getFocusedRowIndex(), "ROUT_ID"));
		$p.executeSubmission(sub_VH0105G1R0);
		
	};
	
	scwin.sub_VH0105G0R0_submitdone = function(e) {
		com.setFocusedCell(VH0105G0, "OPER_DT");
		var operSts = "";
		
		for (i=0; i<dlt_BRT_CUR_OPER_HIS.getDataLength(); i++) {
			operSts = dlt_BRT_CUR_OPER_HIS.getCellData(i, "OPER_STS");
			
			switch(operSts) {
				//정상운행	
				case "OS001":
				VH0105G0.setCellClass(i, "OPER_STS", "color_div1");
				break;
				
				//운행종료
				case "OS002":
				VH0105G0.setCellClass(i, "OPER_STS", "color_div2");
				break;
				
				//차고지행
				case "OS003":
				VH0105G0.setCellClass(i, "OPER_STS", "color_div3");
				break;
			}
			
		}
	};

	scwin.sub_repRoutItem_submitdone = function(e) {
		com.executeSubmission(sub_routItem);
	};

	scwin.sub_VH0105G1R0_submitdone = function(e) {
		routMap.showRoute("map_VH0105", dlt_BMS_ROUT_NODE_CMPSTN.getAllJSON(), null, "ONLYLINE");
		
		var focusIdx = VH0105G0.getFocusedRowIndex();
		var focusDataList = dlt_BRT_CUR_OPER_HIS.getRowJSON(focusIdx);
		
		routMap.showVehicle3("map_VH0105", focusDataList,  VH0105G0);
	};
	
	scwin.TYPE1_onviewchange = function(info) {	
		com.executeSubmission(sub_routItem);
		//com.searchGrid(VH0105G0, sub_VH0105G0R0);
	};
	
	/*scwin.TYPE2_onviewchange = function(info) {		

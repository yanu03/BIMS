			scwin.sub_VH0206G0R0_submitdone = function(e) {
				com.setFocusedCell(VH0206G0, "VHC_ID");
				var hisDiv = "";
				var phaseLen = "";
				var periodCnt = "";
				
				
				for (i=0; i<dlt_BMS_DVC_COND_PARAM_INFO_HIS.getDataLength(); i++) {
					var phaseLenMin = "";
					var phaseLenSec = "";
					var periodCntMin = "";
					var periodCntSec = "";	
			
					phaseLen = dlt_BMS_DVC_COND_PARAM_INFO_HIS.getCellData(i, "PERIOD_LEN");
					periodCnt = dlt_BMS_DVC_COND_PARAM_INFO_HIS.getCellData(i, "PERIOD_CNT");
					
					if((parseInt(phaseLen) >= 60)) {
						phaseLenMin = parseInt(phaseLen/60) + "분 ";
					}
					
					phaseLenSec = parseInt(phaseLen%60) + "초";
					phaseLen = phaseLenMin + phaseLenSec;
					
					
					if((parseInt(periodCnt) >= 60)) {
						periodCntMin = parseInt(periodCnt/60) + "분 ";
					}
					
					periodCntSec = parseInt(periodCnt%60) + "초";
					periodCnt = periodCntMin + periodCntSec;	
					
					dlt_BMS_DVC_COND_PARAM_INFO_HIS.setCellData(i, "PERIOD_LEN", phaseLen);
					dlt_BMS_DVC_COND_PARAM_INFO_HIS.setCellData(i, "PERIOD_CNT", periodCnt);	
				
				
					hisDiv = dlt_BMS_DVC_COND_PARAM_INFO_HIS.getCellData(i, "HIS_DIV");
					
					switch(hisDiv) {
						case "HD001":
						VH0206G0.setCellClass(i, "HIS_DIV", "color_div1");
						break;
						
						case "HD002":
						VH0206G0.setCellClass(i, "HIS_DIV", "color_div2");
						break;
						
						case "HD003":
						VH0206G0.setCellClass(i, "HIS_DIV", "color_div3");
						break;
					}
					
				}
			};
		

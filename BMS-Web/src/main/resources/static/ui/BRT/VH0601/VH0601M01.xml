	scwin.sub_VH0601G0R0_submitdone = function(e) {
		com.setFocusedCell(VH0601G0, "UPD_DTM");
		
		var sigCtrSts = "";
		var sigOperMode = "";
		var phaseLen = "";
		var periodCnt = "";

		
		for (i=0; i<dlt_BMS_CRS_GRP_SIGOPER_HIS.getDataLength(); i++) {
			var phaseLenMin = "";
			var phaseLenSec = "";
			var periodCntMin = "";
			var periodCntSec = "";
						
			sigCtrSts = dlt_BMS_CRS_GRP_SIGOPER_HIS.getCellData(i, "SIG_CTR_STS"); 	
			sigOperMode = dlt_BMS_CRS_GRP_SIGOPER_HIS.getCellData(i, "SIG_OPER_MODE");
			phaseLen = dlt_BMS_CRS_GRP_SIGOPER_HIS.getCellData(i, "PHASE_LEN");
			periodCnt = dlt_BMS_CRS_GRP_SIGOPER_HIS.getCellData(i, "PERIOD_CNT");
			
			if((parseInt(phaseLen) >= 60)) {
				phaseLenMin = parseInt(phaseLen/60) + "분 ";
			}
			
			phaseLenSec = parseInt(phaseLen%60) + "초";
			phaseLen = phaseLenMin + phaseLenSec;
			
			
			if((parseInt(periodCnt) >= 60)) {
				periodCntMin = parseInt(periodCnt/60) + "분 ";
			}
			
			periodCntSec = parseInt(periodCnt%60) + "초";
			periodCnt = periodCntMin + periodCntSec;	
			
			dlt_BMS_CRS_GRP_SIGOPER_HIS.setCellData(i, "PHASE_LEN", phaseLen);
			dlt_BMS_CRS_GRP_SIGOPER_HIS.setCellData(i, "PERIOD_CNT", periodCnt);		
			
			
			switch(sigCtrSts) {
				//OFFLINE
				case "0":
				VH0601G0.setCellClass(i, "SIG_CTR_STS", "cond_abnormal");
				break;
				
				//ONLINE
				case "1":
				VH0601G0.setCellClass(i, "SIG_CTR_STS", "cond_normal");
				break;				
			}
			
			switch(sigOperMode) {
				
				//TOD
				case "0":
				VH0601G0.setCellClass(i, "SIG_OPER_MODE", "color_div1");
				break;				
				
				//TRC
				case "1":
				VH0601G0.setCellClass(i, "SIG_OPER_MODE", "color_div2");
				break;				
				
				//TOD
				case "2":
				VH0601G0.setCellClass(i, "SIG_OPER_MODE", "color_div3");
				break;				
				
			}
			 	
		}
		
	};

	scwin.F_DATE_onviewchange = function(info) {
		var ldate = L_DATE.getValue();
		var fdate = F_DATE.getValue();
		
		if (fdate > ldate && ldate != ""){
			com.messagBox("alert", "지정할 수 없는 날짜입니다.",scwin.checkCLoseF(info));
		}
		
		else{
			calendar_radio.setValue("DIRECT");
		}
		
	};
	scwin.checkCLoseF = function(info) {
		F_DATE.setValue(info.oldValue);
	}
	scwin.checkCLoseL = function(info) {
		L_DATE.setValue(info.oldValue);
	}	
	scwin.L_DATE_onviewchange = function(info) {
		var ldate = L_DATE.getValue();

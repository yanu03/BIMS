		}
	};
	
	//라디오 버튼에 따라 날짜 설정
	scwin.calendar_radio_onviewchange = function(info) {
		var date = new Date();
		var year = date.getFullYear().toString();
		var month = (date.getMonth()+1).toString();
		var day = date.getDate().toString();
		var zero = 0;
		
		//글자수 맞추기
		if(day.length == 1){
			day = zero + day;
		}
		
		if(month.length == 1){
			month = zero + month;
		}
		
		var weekAgoDay = parseInt(day) - 7;
		var todaysDate = year+month+day;
		var weekAgoDate = year+month+weekAgoDay;
		var firstday = year+month+"01";		
		
		if(info.value == "DIRECT") {
			F_DATE.setValue();
			L_DATE.setValue();
		}
		
		else if(info.value == "TODAY") {
			F_DATE.setValue(todaysDate);
			L_DATE.setValue(todaysDate);	
		}
		
		else if(info.value == "WEEK") {
			F_DATE.setValue(weekAgoDate);
			L_DATE.setValue(todaysDate);			
		}
		
		else if(info.value == "MONTH") {
			F_DATE.setValue(firstday);
			L_DATE.setValue(todaysDate);				
		}
	};


	scwin.getFooterDivTop = function(data, dataType) {
		var result = scwin.getEventCount(data, dataType, "top", "SIG_CTR_STS");
		return result;
	};
	
	scwin.getFooterDivBottom = function(data, dataType) {
		var result = scwin.getEventCount(data, dataType, "bottom", "SIG_OPER_MODE");
		return result;
	};	
	
	scwin.getEventCount = function(data, dataType, footerPosition, dataColumn) {
		var sum = 0;
		var count01 = 0;
		var count02 = 0;
		var count03 = 0; 
				
		var stsCount01 = 0;
		var stsCount02 = 0;
		
		var result = "";
		
		if (data.length > 0) {
			sum = data.length;
			for (var i=0; i<data.length; i++) {
			
				if(dataColumn == "SIG_CTR_STS"){
				
					switch(data[i]) {
						case "0":
						stsCount01++;
						break;
						
						case "1":
						stsCount02++;
						break;
						
						case "교통소통":
						count03++;
						break;						
					}			
				}
			
					
				else if(dataColumn == "SIG_OPER_MODE") {
				
					switch(data[i]) {
						case "0":
						count01++;
						break;
						
						case "1":
						count02++;
						break;
						
						case "2":
						count03++;
						break;						
					}					
				}				
			}				
			
			if(footerPosition == 'top') {
				result = "총계 : " + sum + "건, ONLINE : " + stsCount01+ "건, OFFLINE : " + stsCount02 + "건";						
			}
			
			else if (footerPosition == 'bottom') {
				result =  "TOD : " + count01 + "건, TRC : " + count02 + "건, MAN : " + count03 + "건"		
			}
			return result;
		}
	}			
